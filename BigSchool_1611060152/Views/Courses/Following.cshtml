@model IEnumerable<BigSchool_1611060152.Models.Following>
@{
    ViewBag.Title = "Following";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Flowing</h2>
<ul class="following">
    @foreach (var following in Model)
    {
        <li>
           
            <div class="details">
                <span>
                    @following.Followee.Name
                </span>
                

            </div>
        </li>
    }
</ul>


